<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0.0" package="com.m039.ibeacon.keeper.foreground_sample">

  <uses-sdk android:minSdkVersion="18" android:targetSdkVersion="19"/>

  <uses-permission android:name="android.permission.BLUETOOTH"/>
  <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>

  <uses-feature android:name="android.hardware.bluetooth_le" android:required="true"/>

  <application android:allowBackup="true"
               android:icon="@drawable/ic_launcher"
               android:label="@string/app_name"
               android:name="com.m039.ibeacon.keeper.app.MainApplication">

    <activity android:name="com.m039.ibeacon.keeper.activity.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>

    <service android:name="com.m039.ibeacon.keeper.service.IBeaconService"
             android:process=":com.m039.ibeacon.keeper.service.IBeaconService"
             />

  </application>

</manifest>
